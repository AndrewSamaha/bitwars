{
  "name": "bitwars",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:no-turbo": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "db:init": "curl -X POST http://localhost:3000/api/init",
    "createmock:gamestate": "curl -X POST http://localhost:3000/api/gamestate/mocks/create_gamestate",
    "createmock:players": "curl -X POST http://localhost:3000/api/players/mocks/create_players",
    "pretest": "pnpm -C ../../packages/shared build && cargo run --example generate_examples --manifest-path ../../services/rts-engine/Cargo.toml",
    "test": "vitest run",
    "test:read-redis": "tsx test/read_redis.ts",
    "test:write-and-read-redis": "tsx test/write_and_read_redis.ts",
    "test:write-and-read-ioredis": "tsx test/write_and_read_ioredis.ts",
    "ioredis-version": "ioredis --version"
  },
  "dependencies": {
    "@bitwars/shared": "workspace:*",
    "@bufbuild/protobuf": "^2.7.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "geist": "^1.4.2",
    "ioredis": "^5.7.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "miniplex": "^2.0.0",
    "next": "15.5.2",
    "pixi.js": "^8.13.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "redis": "^5.8.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.4.5",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "vitest": "^2.1.4"
  }
}
