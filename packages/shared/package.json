{
  "name": "@bitwars/shared",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "sideEffects": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./gen/*": {
      "types": "./dist/gen/*.d.ts",
      "import": "./dist/gen/*.js",
      "default": "./dist/gen/*.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.7.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.5.0"
  }
}
